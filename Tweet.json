      "parameters": {
        "jsCode": "for (const item of $input.all()) {\n  // 1. Get the messy text from the 'output' field\n  let rawText = item.json.output || \"\";\n\n  // 2. Clean it:\n  // - Removes (1/10), (2/10), etc.\n  // - Removes the literal strings '\\n \\n' or actual newlines\n  let cleaned = rawText\n    .replace(/\\(\\d+\\/\\d+\\)/g, '') // Removes the (number/number) part\n    .replace(/\\\\n/g, ' ')         // Removes literal backslash-n characters\n    .replace(/\\s+/g, ' ')         // Turns multiple spaces/newlines into one space\n    .trim();                      // Removes leading/trailing spaces\n\n  // 3. Save it to your 'text_cleaned' field\n  item.json.text_cleaned = cleaned;\n}\n\nreturn $input.all();"
      },
