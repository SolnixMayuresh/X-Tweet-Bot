    {
      "parameters": {
        "jsCode": "// Input text from your previous step\nconst rawText = $input.first().json.text; \n\nconst parseData = (text) => {\n    // Split text into lines and remove empty ones\n    const lines = text.split('\\n').map(line => line.trim()).filter(line => line !== \"\");\n    \n    const result = [];\n    let currentTopic = \"\";\n    let currentDescriptions = [];\n\n    lines.forEach((line) => {\n        if (!line.startsWith('-')) {\n            // If we encounter a new topic, save the previous one first\n            if (currentTopic !== \"\") {\n                result.push({\n                    topic: currentTopic,\n                    description: currentDescriptions.join('\\n')\n                });\n            }\n            // Reset for the new topic\n            currentTopic = line;\n            currentDescriptions = [];\n        } else {\n            // Add the bullet point to the description list\n            currentDescriptions.push(line);\n        }\n    });\n\n    // Push the very last item in the list\n    if (currentTopic !== \"\") {\n        result.push({\n            topic: currentTopic,\n            description: currentDescriptions.join('\\n')\n        });\n    }\n\n    return result;\n};\n\n// This returns the array of objects directly\nreturn parseData(rawText);"
      },
